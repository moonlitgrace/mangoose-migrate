{
  "name": "mangoose-migrate",
  "version": "0.0.1",
  "description": "A lightweight migration tool for Mongoose (MongoDB), inspired by Django's migration system.",
  "keywords": [
    "mongoose",
    "mongodb",
    "migrations",
    "schema",
    "cli"
  ],
  "homepage": "https://github.com/moonlitgrace/mangoose-migrate#readme",
  "bugs": {
    "url": "https://github.com/moonlitgrace/mangoose-migrate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonlitgrace/mangoose-migrate.git"
  },
  "license": "MIT",
  "author": "moonlitgrace",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mangoose-migrate": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./core": {
      "require": "./dist/core/index.cjs",
      "types": "./dist/core/index.d.cts"
    },
    "./operations": {
      "require": "./dist/operations/index.cjs",
      "types": "./dist/operations/index.d.cts"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "changeset": "changeset",
    "release:version": "changeset version",
    "release:publish": "npm run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^22.15.21",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "mongoose": "^8.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
